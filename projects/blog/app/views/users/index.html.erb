<p style="color: green"><%= notice %></p>
<%= render 'layouts/header' %>
<h1>Users</h1>

<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li class="user">
      <%= render user %>
      <%= link_to "Show this user", user %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>

<%= link_to "New user", new_user_path %>
<%= render 'layouts/footer' %>